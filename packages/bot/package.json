{
  "name": "@tego/bot",
  "version": "0.2.1",
  "description": "High-performance desktop automation library for Node.js, powered by Rust",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "build": "napi build --release --platform --esm --const-enum",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "test": "cargo test"
  },
  "keywords": [
    "automation",
    "robot",
    "mouse",
    "keyboard",
    "screenshot",
    "desktop",
    "rust",
    "napi",
    "electron"
  ],
  "author": {
    "name": "sealday",
    "email": "sealday@gmail.com",
    "url": "https://github.com/sealday"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tegojs/bot.git"
  },
  "homepage": "https://github.com/tegojs/bot#readme",
  "bugs": {
    "url": "https://github.com/tegojs/bot/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@napi-rs/cli": "^3.1.5"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "binaryName": "bot",
    "packageName": "@tego/bot",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "i686-pc-windows-msvc",
      "aarch64-pc-windows-msvc"
    ]
  },
  "engines": {
    "node": ">= 20"
  }
}

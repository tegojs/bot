{
  "name": "@tego/botjs",
  "version": "0.2.2",
  "description": "Type-safe TypeScript wrapper for Tego Bot - High-performance desktop automation library powered by Rust",
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "ENABLE_INTEGRATION_TESTS=true vitest run",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "pnpm dlx serve docs"
  },
  "dependencies": {
    "@tego/bot": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.9",
    "tsdown": "^0.16.6",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tegojs/bot.git"
  },
  "homepage": "https://github.com/tegojs/bot#readme",
  "bugs": {
    "url": "https://github.com/tegojs/bot/issues"
  },
  "license": "MIT",
  "author": {
    "name": "sealday",
    "email": "sealday@gmail.com",
    "url": "https://github.com/sealday"
  },
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "files": [
    "dist",
    "readme.md",
    "package.json"
  ],
  "engines": {
    "node": ">= 20"
  }
}

{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "../../docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "name": "Tego Bot API Documentation",
  "includeVersion": true,
  "readme": "none",
  "categorizeByGroup": true,
  "categoryOrder": ["Classes", "Functions", "Interfaces", "Type Aliases", "*"],
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/tests/**",
    "**/*.test.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "cleanOutputDir": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "sort": ["source-order", "required-first", "alphabetical"],
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "hideGenerator": false
}
